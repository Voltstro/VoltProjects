{
  "VoltProjects": {
    "SitesBuildDir": "Temp/",
    "SitesServingDir": "Sites/",
    "SitesRebuildTimeSeconds": 1209600,
    "SitesServingCacheHeaderTimeSeconds": 1209600,
    "Projects": [
      {
        "Name": "UnityWebBrowser",
        "NameShort": "UWB",
        "GitPath": "https://github.com/Voltstro-Studios/UnityWebBrowser",
        "GitBranch": "master",
        "GitUseLatestTag": false,
        "IconPath": "media/UWB-Icon.svg",
        "DocsPath": "docs/",
        "DocsBuildDist": "_site/UnityWebBrowser/"
      },
      {
        "Name": "VoltRpc",
        "GitPath": "https://github.com/Voltstro-Studios/VoltRpc",
        "GitBranch": "master",
        "GitUseLatestTag": false,
        "IconPath": "media/icon.svg",
        "DocsPath": "docs/",
        "DocsBuildDist": "_site/VoltRpc/"
      },
      {
        "Name": "WikiNet",
        "GitPath": "https://github.com/Voltstro-Studios/Wiki.Net",
        "GitBranch": "master",
        "GitUseLatestTag": false,
        "IconPath": "icon.png",
        "DocsPath": "docs/",
        "DocsBuildDist": "_site/WikiNet/"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "AllowedHosts": "localhost"
}
