{
  "VoltProjects": {
    "SitesBuildDir": "Temp/",
    "SitesServingDir": "Sites/",
    "SitesUpdateTime": 1209600,
    "HostCacheTime": 1209600,
    "Projects": [
      {
        "Name": "VoltRpc",
        "GitUrl": "https://github.com/Voltstro-Studios/VoltRpc",
        "GitBranch": "master",
        "GitUseLatestTag": false,
        "DocsPath": "docs/",
        "DocsBuildDist": "_site/"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information"
  },
  "Sentry": {
    "Debug": false,
    "TracesSampleRate": 1.0
  },
  "AllowedHosts": "localhost"
}
