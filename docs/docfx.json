{
    "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
    "build": {
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "src": "../",
                "files": [
                    "CHANGELOG.md"
                ]
            },
            {
                "src": "articles",
                "files": [
                    "toc.yml",
                    "**/*.md"
                ],
                "dest": "articles"
            }
        ],
        "resource": [
            {
                "files": [
                    "assets/**/*"
                ]
            }
        ],
        "output": "_site",
        "template": [
            "default"
        ],
        "globalMetadata": {
            "_appName": "voltprojects",
            "_appTitle": "voltprojects",
            "_enableSearch": false,
            "pdf": false
        }
    }
}